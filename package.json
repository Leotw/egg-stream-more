{
  "name": "egg-stream-more",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "declarations": true
  },
  "scripts": {
    "clean": "easy clean",
    "build": "cross-env EGG_SERVER_ENV=prod NODE_ENV=production easy build --devtool",
    "build:prod": "cross-env EGG_SERVER_ENV=prod NODE_ENV=production easy build --devtool",
    "build:stage": "cross-env EGG_SERVER_ENV=stage NODE_ENV=production easy build --devtool",
    "build:local": "cross-env EGG_SERVER_ENV=local NODE_ENV=production easy build --devtool",
    "start": "EGG_SERVER_ENV=prod egg-scripts start --daemon --title=egg-server-egg-stream-more",
    "start:prod": "EGG_SERVER_ENV=prod egg-scripts start --daemon --title=egg-server-egg-stream-more",
    "start:stage": "EGG_SERVER_ENV=stage egg-scripts start --daemon --title=egg-server-egg-stream-more",
    "start:local": "EGG_SERVER_ENV=local egg-scripts start --daemon --title=egg-server-egg-stream-more",
    "stop": "egg-scripts stop --title=egg-server-egg-stream-more",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run cov",
    "autod": "autod",
    "deploy": "bash .sh/deploy.sh"
  },
  "dependencies": {
    "@sentry/browser": "^5.10.2",
    "@sentry/node": "^5.10.2",
